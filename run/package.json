{
  "name": "restyled-run",
  "version": "0.0.0",
  "type": "module",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build lib/main.js && sed -i 's/\\x0D$//' ./dist/index.js",
    "format": "prettier --write \"**/*.ts\"",
    "format-check": "prettier --check \"**/*.ts\"",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "lint": "eslint",
    "readme": "npx action-docs -u && prettier --write README.md"
  },
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/exec": "^3.0.0",
    "@actions/github": "^9.0.0",
    "@types/temp": "^0.9.4",
    "action-docs": "^2.5.1",
    "temp": "^0.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
